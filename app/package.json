  "main": "index.js",
  "scripts": {
    "build:local": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:production": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multiples": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-style-loaders/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:server": "webpack-dev-server --config ./dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:precss": "webpack-dev-server --config ./precss/webpack.config.js",
    "build:dupl": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:prevent-duplication-fast": "webpack --config ./dll/webpack.config.js",
    "build:dynamic:dll": "webpack --config ./dynamic-imports/webpack.dll.config.js",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js",
    "build:badges": "webpack --config ./platzi-badges/webpack.config.js --mode production",
     "build:badges:dll": "webpack --config ./platzi-badges/webpack.dll.config.js --mode production"
  },
  "keywords": [
    "webpack"
  ],
