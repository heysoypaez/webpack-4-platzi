    "dynam": "1.2.1",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "4.12.0",
